Person type = (
	[]i8 name,
	i32 age,
)

Token type =
	| unit binop
	| unit unop
	| unit number

Result type =
	| unit ok
	| []i8 err

age_up fn (Person p) -> () = {
	v0 = mul.i32 p.age 2
	v1 = add.i32 v0 1
	store.i32 p.age v1
}





